dimension :: 2
axes :: x,y

d_xx = fun(q) (q[i+1,j] + q[i-1,j] - 2*q[i,j])/dx**2
d_yy = fun(q) (q[i,j+1] + q[i,j-1] - 2*q[i,j])/dy**2
double :: dt = 0.1*dx*dx

extern function :: exp
gauss = fun(x,y) exp(-((x-(length_x/2))/(5*dx))**2 - ((y-(length_y/2))/(5*dy))**2)

begin function q = init()
  double [] :: q = 0
  q[i,j] = gauss(i*dx,j*dy)
end function

begin function q_next = step(q)
  q_xx = d_xx(q)
  q_yy = d_yy(q)
  q_next = q + dt*(q_xx + q_yy)
end function
